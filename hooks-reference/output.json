[
  {
    "type": "hook",
    "name": "AdminAreaFooterOutput",
    "description": "Runs on every admin area page load. All template variables defined at the time the hook is invoked are made available to this hook point. This can vary by page. The list below is not an exhaustive list.",
    "parameters": [
      {
        "Variable": "charset",
        "Type": "string",
        "Notes": "System charset setting eg. UTF-8"
      },
      {
        "Variable": "template",
        "Type": "string",
        "Notes": "The admin template being used"
      },
      {
        "Variable": "pagetemplate",
        "Type": "string",
        "Notes": "The template for the current page (if applicable)"
      },
      {
        "Variable": "adminid",
        "Type": "int",
        "Notes": "The currently logged in admin user ID"
      },
      {
        "Variable": "admin_username",
        "Type": "string",
        "Notes": "The username of the current admin user"
      },
      {
        "Variable": "admin_notes",
        "Type": "string",
        "Notes": "The private notes for the current admin user"
      },
      {
        "Variable": "admin_perms",
        "Type": "array",
        "Notes": "The permissions of the current admin user"
      },
      {
        "Variable": "addon_modules",
        "Type": "array",
        "Notes": "A list of add-on modules the current admin user has access to"
      },
      {
        "Variable": "filename",
        "Type": "string",
        "Notes": "The name of the file being called"
      },
      {
        "Variable": "pagetitle",
        "Type": "string",
        "Notes": "The title of the current page"
      },
      {
        "Variable": "helplink",
        "Type": "string",
        "Notes": "The documentation url for the current page"
      },
      {
        "Variable": "sidebar",
        "Type": "string",
        "Notes": "The sidebar output"
      },
      {
        "Variable": "minsidebar",
        "Type": "bool",
        "Notes": "Returns true if the sidebar is minimised"
      },
      {
        "Variable": "jquerycode",
        "Type": "string",
        "Notes": "jQuery code for the current page"
      },
      {
        "Variable": "jscode",
        "Type": "string",
        "Notes": "Javascript code for the current page"
      },
      {
        "Variable": "datepickerformat",
        "Type": "string",
        "Notes": "The format defined for dates in the admin area"
      },
      {
        "Variable": "adminsonline",
        "Type": "string",
        "Notes": "A list of currently online admin users"
      }
    ],
    "source_file": "hooks-reference/output.md"
  },
